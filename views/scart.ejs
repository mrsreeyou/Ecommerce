<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sreeyou Product Page</title>
   <!-- <link rel="stylesheet" href="scart.css"> -->
</head>

<body>
    <header class="header">
        <!-- <a href="/admin/dashboard" class="admin-link">Admin Dashboard</a> -->
        <h1 class="user-welcome">
            <%= welcome %>
        </h1>
        <nav class="nav">
            <ul>
                <li><a href="/user/scart">Home</a></li>
                <li><a href="/products">Products</a></li>
                <li><a href="/about">About</a></li>
                <li><a href="/contact">Contact</a></li>
                <li><a href="/user/cart">Cart</a></li>
                <li><a href="/user/wishlist">Wishlist</a></li>
                <li><a href="/user/orders/place">Orders</a></li>
            </ul>
        </nav>
       <% if(!token){ %>
        <div class="auth-buttons">
            <a href="/user/login">Login</a>
        </div>
        <% }else{ %>
            <div class="auth-buttons">
                <a href="/user/logout">Logout</a>
            </div>
            <%} %>
        
    </header>
   
    <main class="main">
        <!-- Filter Form -->
         <div class="filter-form">
        <form action="/user/products/filter" method="GET">
            <label for="category">Category:</label>
            <select name="category" >
                <% categories.forEach(category => { %>
                
                     <option value="<%= category._id %>"><%= category.name %></option>
                    
                    
                <% }) %>
            </select>
            
            <label for="minPrice">Min Price:</label>
            <input type="number" id="minPrice" name="minPrice" placeholder="0" >
            
            <label for="maxPrice">Max Price:</label>
            <input type="number" id="maxPrice" name="maxPrice" placeholder="100000" >
            
            <label for="status">Status:</label>
            <select id="status" name="status">
                <option value="">All</option>
                <option value="active">>Active</option>
                <option value="inactive" >>Inactive</option>
            </select>
    
            <button type="submit" class="btn btn-filter">Filter</button>
            
            
        </form>
    </div>
<!--image scroll-->
    <div class="image-scroller">
        <div class="scroll-container">
            <img src="https://www.shutterstock.com/shutterstock/photos/552433030/display_1500/stock-vector-moisturizing-cosmetic-products-ad-light-blue-bokeh-background-with-beautiful-containers-and-watery-552433030.jpg" alt="Ad 1">
            <img src="https://www.creatopy.com/blog/wp-content/uploads/2022/06/hersheys_chocolate_display_ad.jpg" alt="">
            <img src="https://queue-it.com/media/ppcp1twv/product-drop.jpg" alt="Ad 2">
            <img src="data:image/jpeg;base64," alt="Ad 3">
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT9FhNRD7PDPgMpkD_xWFSlvJoMrFWvRcnhCA&s" alt="Ad 4">
            <img src="data:image/jpeg;base64," alt="">
         
            <img src="https://study.com/cimages/multimages/16/burgerad15179945781952220614.png" alt="">
            <img src="" alt="">
            <img src="" alt="">
        </div>
    </div>
    
        <!-- Product List -->
        <div class="products-row">
            <% products.forEach(product => { %>
                <% if (!product.isBlocked) { %>
                    <div class="product-container">
                        <div class="product-details">
                            <img src="/uploads/<%= product.image %>" alt="<%= product.name %>" class="product-image">
                            <div class="product-info">
                                <h2 class="product-title"><%= product.name %></h2>
                                <p class="product-price">Price: $<%= product.price %></p>
                                <a href="/user/views/products/details/<%= product._id %>"> <p class="viewdetails">view Details ></p></a>
                            </div>
                        </div>
                    </div>
                <% } %>
            <% }) %>
        </div>
    </main>
    

    <footer class="footer">
        <div class="container">
            <p>&copy; 2024 Scart. All rights reserved.</p>
            <p>@mr.sreeyou</p>
        </div>
    </footer>
</body>
<style>
    /* Styles for the Sreeyou Product Page */

body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    background-color: #f9f9f9;
    color: #333;
}

.container {
    width: 90%;
    max-width: 1200px;
    margin: 0 auto;
}

.header {
    background-color: #24272b;
    color: white;
    padding: 1rem 2rem; /* Added padding on sides for better spacing */
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: fixed; /* Makes the header sticky */
    top: 0; /* Ensures it sticks to the top */
    width: 100%; /* Ensures it spans the full width */
    z-index: 1000; /* Keeps it above other elements */
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); /* Adds a subtle shadow for better visibility */
}

.logo {
    font-size: 1.5rem;
    font-weight: bold;
    margin: 0;
}

.admin-link {
    position: absolute;
    top: 50%;
    left: 1rem;
    transform: translateY(-50%);
    background-color: white;
    color: #007BFF;
    text-decoration: none;
    padding: 0.5rem 1rem;
    border-radius: 5px;
    font-weight: bold;
    font-size: 0.9rem;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
}

.admin-link:hover {
    background-color: #0056b3;
    color: white;
}

.nav ul {
    display: flex;
    justify-content: center;
    list-style: none;
    padding: 0;
    margin: 0;
}

.nav li {
    margin: 0 1rem;
}

.nav a {
    color: white;
    text-decoration: none;
    font-weight: bold;
}
.nav li :hover{
    color: rgb(17, 81, 201);
}

.auth-buttons {
    display: flex;
    gap: 1rem;
}

.auth-buttons a {
    color: white;
    text-decoration: none;
    font-weight: bold;
    padding: 0.5rem 1rem;
    border-radius: 5px;
}

.auth-buttons a:hover {
    background-color: #303233;
}

.main {
    padding: 8rem 0;
}

.products-row {
    display: flex;
    flex-wrap: wrap;
    gap: 1px;
    justify-content: center;
    padding-top: 10px;
}

.product-container {
    text-align: center;
    background-color: white;
    border-radius: 10px;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    padding: 2rem;
    flex: 1 1 calc(30% - 1rem);
    max-width: calc(30% - 1rem);
}

.product-title {
    font-size: 1.5rem;
    margin-bottom: 1rem;
}

.product-details {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 1.5rem;
}

.product-image {
    width: 150px;
    height: auto;
    border-radius: 10px;
}

.product-info {
    text-align: left;
}

.product-description {
    font-size: 1rem;
    margin-bottom: 1rem;
}

.product-price {
    font-size: 1.2rem;
    font-weight: bold;
    color: #007BFF;
    display: flex;
    justify-content: center;
}

.btn {
    display: inline-block;
    padding: 0.75rem 1.5rem;
    border: none;
    border-radius: 5px;
    background-color: #2c3035;
    color: white;
    font-size: 1rem;
    cursor: pointer;
    text-transform: uppercase;
    font-weight: bold;
}
.btn:hover {
    background-color: #3c3c3d;
}
.btn2 {
    padding-left: 50%;
    padding: 0.75rem 1.5rem;
    border: none;
    border-radius: 5px;
    background-color: #01080f;
    color: white;
    font-size: 1rem;
    cursor: pointer;
    text-transform: uppercase;
    font-weight: bold;
}

.btn2:hover {
    background-color: #0056b3;
}

.footer {
    background-color: #333;
    color: white;
    text-align: center;
    padding: 1rem 0;
    margin-top: 2rem;
}

.user-welcome {
    color: #f9f9f9;
}
.filter-form{
    background-color: #d5dadf;
    display: flex;
    justify-content: center;
    
}
.viewdetails{
    display: flex;
    justify-content: center;
}

.image-scroller {
overflow: hidden;
width: auto;
height: auto;
background-color: #f9f9f9; /* Adjust background as needed */
padding: 20px 0;
margin-bottom: 2rem;
position: relative;
background-color: #007BFF;
}

.scroll-container {
display: flex;
gap: 20px;
animation: scroll-alternate 10s linear infinite;
will-change: transform;
}

.scroll-container img {
width: 100%; /* Adjust image width */
height: 250px;
border-radius: 10px;
box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
}

@keyframes scroll-alternate {
0% {
transform: translateX(0%);
}
50% {
transform: translateX(-50%);
}
100% {
transform: translateX(0%);
}
}
</style>
</html>